<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectPictureComponent">
        <attr name="text" format="string"/>
        <attr name="icon" format="reference"/>
        <attr name="target" format="enum">
            <enum name="image" value="0"/>
            <enum name="camera" value="1"/>
            <enum name="video" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LoginButton">
        <attr name="type" format="enum">
            <enum name="google" value="0" />
            <enum name="facebook" value="1" />
            <enum name="twitter" value="2" />
        </attr>
    </declare-styleable>
</resources>